{
  "name": "No more nagelsvamp!",
  "version": "1",
  "description": "Removes nagelsvamp from Disqus!",
  "background": {"page": "background.html"},
  "manifest_version": 2,
  "browser_action": {
    "name": "Manipulate DOM",
    "icons": ["icon.png"],
    "default_icon": "icon.png"
  },
  "content_scripts": [ {
    "js": [ "jquery.js", "background.js" ],
    "all_frames": true,    
    "matches": [ "http://*/*", "https://*/*"]
  }]
}